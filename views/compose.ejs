<%- include('./partials/header'); -%>

<body>

<h1>Compose</h1>

  <form class="" action="/compose" method="post">

    <div class="form-group">
      <label for="title">Title</label>
      <textarea class="form-control" id="title" rows="1" name="title"></textarea>
    </div>

    <div class="form-group">
      <label for="textarea">Post</label>
      <textarea class="form-control" id="textarea" rows="5" name="post"></textarea>
    </div>

    <button class="btn btn-primary" type="submit" name="button">Publish</button>
  </form>
</body>

